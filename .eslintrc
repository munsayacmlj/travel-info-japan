  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2015,
    "allowImportExportEverywhere": true
  },  
  "env": {
    "browser": true
  },
  "globals": {
    "browser": true,
    "console": true
